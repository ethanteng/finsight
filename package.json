{
  "name": "finsight",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:unit": "jest --testPathPattern=unit",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "build:backend": "NODE_OPTIONS='--max-old-space-size=4096' npx tsc --outDir ./dist && echo 'TypeScript compilation completed' && ls -la dist/",
    "build:prisma": "prisma generate",
    "build": "./build.sh",
    "build:render": "NODE_OPTIONS='--max-old-space-size=2048' prisma generate && prisma migrate deploy && npm run build:backend",
    "start": "node dist/index.js",
    "dev:backend": "ts-node src/index.ts",
    "dev:frontend": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.1.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "concurrently": "^9.2.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@types/cors": "^2.8.19",
    "@types/node-cron": "^3.0.11",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "node-cron": "^4.2.1",
    "openai": "^5.10.2",
    "plaid": "^37.0.0",
    "prisma": "^6.12.0",
    "react-plaid-link": "^4.0.1"
  }
}
